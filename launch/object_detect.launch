<launch>
	<node pkg= "tf_object_detect" name="detect_ros" type="detect_ros.py"  output="screen"> 
    <remap from='image' to='/camera/rgb/image_raw'/>
	</node>
	<!-- rviz -->
  <arg name ="rviz" default="true" />
  <arg name ="rviz_file" default="$(find tf_object_detect)/config/display.rviz"/>
	<node pkg ="rviz" type="rviz" name="rviz" output="screen" args= "-d $(arg rviz_file)" if="$(arg rviz)"/>
  <include file="$(find openni2_launch)/launch/openni2.launch"/>
</launch>
